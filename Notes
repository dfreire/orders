Product(name)
	Variant(name, weight, availableQuantity, sku, minOrderQuantity, minOrderQuantityIncrement, vatRates)
		BasePrice(value, shippingRegions)
		PromoPrice(value, shippingRegions)

ShippingRegion(name)
	ShippingRateFlat(flatValue)
	ShippingRateByWeight(fromWeight, toWeight, value)
	ShippingRateByQuantity(fromQuantity, toQuantity, value)
	ShippingRateByTotalPrice(fromTotalPrice, toTotalPrice, value)

VatRate(name, value, shippingRegions)

Customer(name, email, password)
	Address(
		fullName, streetLine1, streetLine2, streetLine3, postCode, townOrCity, country
		isBillingAddress, isDefaultBillingAddress, vatNumber, companyName,
		isBillingAddress, isDefaultShippingAddress, shippingRegion, phoneNumber
	)

ShoppingCart(customerEmail)
	ShoppingCartItem(variantId, quantity)

Order(status, date, customerId, copyOfShippingAddress, copyOfBillingAddress)
	ShippingGroup(copyOfShippingRate)
		ShippingGroupItem(
			producId, productName,
			variantId, variantName, variantWeight, variantSKU,
			quantity, basePrice, promoPrice, copyOfVatRate
		)

checkout(shoppingCart) order
